views:
  - type: cards
    name: Orphans Card View
    filters:
      and:
        - "!file.links"
        - "!file.backlinks"
        - '!file.path.containsAny("4 ðŸ“¦ Archive")'
    sort:
      - property: file.mtime
        direction: DESC
    cardSize: 230
  - type: table
    name: Table
    filters:
      and:
        - file.links.isEmpty()
    sort:
      - property: file.name
        direction: ASC
  - type: table
    name: View
