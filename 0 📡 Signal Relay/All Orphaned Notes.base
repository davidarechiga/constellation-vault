views:
  - type: table
    name: Table
    filters:
      and:
        - file.links.isEmpty()
    sort:
      - property: file.name
        direction: ASC
  - type: cards
    name: Orphans Card View
    filters:
      and:
        - "!file.links"
        - "!file.backlinks"
    sort:
      - property: file.mtime
        direction: DESC
    cardSize: 230
  - type: table
    name: View
